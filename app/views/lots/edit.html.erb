<%= form_with url: "/lots/#{@lot.id}", method: :patch, local: true do |form| %>
<%= form.label :name %>
<%= form.text_field :name, value: @lot.name %><br>
<%= form.label :lot_number %>
<%= form.text_field :lot_number, value: @lot.lot_number %><br>
<%= form.label :importer %>
<%= form.text_field :importer, value: @lot.importer %><br>
<%= form.label :organic %>
<%= form.check_box :organic%><br>
<%= form.submit %>
<% end %>
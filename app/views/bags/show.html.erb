<h1><%= @bag.name %></h1>
<p>Ground:
  <%= @bag.ground %></p>
<p>Bag Size:
  <%= @bag.size %></p>
<p>Bag ID:
  <%= @bag.id %></p>
<p>Created:
  <%= @bag.created_at %></p>
<p>Updated:
  <%= @bag.updated_at %></p>

<%= link_to "Update Bag", "/bags/#{@bag.id}/edit", class: "btn btn-warning btn-xs p-0" %>
<%= link_to "Delete Bag" , "/bags/#{@bag.id}" , method: :delete, class: "btn btn-danger btn-xs p-0"%>